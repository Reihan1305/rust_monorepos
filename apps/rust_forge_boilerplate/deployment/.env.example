# Development Environment Variables

# Server
APP__SERVER__HOST=0.0.0.0
APP__SERVER__PORT=8080

# Database
APP__DATABASE__URL=postgresql://user:password@postgres:5432/rust_forge_db
APP__DATABASE__MAX_CONNECTIONS=10

# Redis
APP__REDIS__URL=redis://redis:6379

# MongoDB
APP__MONGODB__URL=mongodb://mongodb:27017
APP__MONGODB__DATABASE=rust_forge_db

# Logging
RUST_LOG=info
